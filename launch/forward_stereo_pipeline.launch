<launch>
  
  <node machine="fugu-c" pkg="camera1394stereo" type="camera1394stereo_node" 
        name="forward_stereo_camera_node" output="screen" >
    <rosparam file="$(find fugu_configurations)/camera_comp_narrow/params.yaml" />
    <param name="frame_id" value="forward_stereo_camera" />
    <remap from="stereo_camera" to="forward_stereo_camera" />
  </node>
  
  <node machine="fugu-c" pkg="stereo_image_proc" type="stereo_image_proc" output="screen"
        name="stereo_image_proc"
        ns="forward_stereo_camera" />
  
</launch>
